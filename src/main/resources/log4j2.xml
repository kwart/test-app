<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR">
    <Appenders>
        <Console name="Console" target="SYSTEM_ERR">
            <PatternLayout pattern="%d{ABSOLUTE} %5p [%c{1}] %t - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="Console" />
        </Root>
<!-- 
        <Logger name="com.hazelcast" level="ALL" />
 -->
    </Loggers>
</Configuration>
